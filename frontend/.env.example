# Internet Computer Canister IDs
# These will be populated after deploying the canisters
REACT_APP_AUTH_CANISTER_ID=
REACT_APP_ASSET_CANISTER_ID=
REACT_APP_MARKETPLACE_CANISTER_ID=

# Internet Identity Canister (use local for development)
REACT_APP_INTERNET_IDENTITY_CANISTER_ID=rdmx6-jaaaa-aaaah-qdrpq-cai

# Environment
REACT_APP_NODE_ENV=development

# API Configuration
REACT_APP_IC_HOST=http://localhost:8000
REACT_APP_IC_NETWORK=local

# Asset Storage Configuration (if using external storage)
REACT_APP_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/
REACT_APP_STORAGE_TYPE=canister

# Application Configuration
REACT_APP_APP_NAME=VR Marketplace
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_VR=true
REACT_APP_ENABLE_WEBXR=true
REACT_APP_ENABLE_ANALYTICS=false

# Development Configuration
GENERATE_SOURCEMAP=true
REACT_APP_DEBUG=true

# Plug Wallet Configuration
REACT_APP_PLUG_WALLET_WHITELIST=localhost:8000,127.0.0.1:8000